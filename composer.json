{
    "name": "compasschurch/elgg-cbcoverseas",
    "type": "elgg-plugin",
    "description": "Elgg theme and modifications specific to cbcoverseas.org",
    "extra": {
        "installer-name": "cbcoverseas"
    },
    "autoload": {
        "psr-4": {
            "Cbcoverseas\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Evan Winslow",
            "email": "evan.b.winslow@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "arckinteractive/elgg_hybridauth": "~1.2",
        "composer/installers": "~1.0",
        "elgg/elgg": "~1.9",
        "ewinslow/elgg-evan": "dev-master",
        "ewinslow/elgg-google": "dev-master",
        "ewinslow/elgg-uservalidationbyadmin": "dev-master",
        "php": "~5.4",
        "srokap/db_backup": "dev-master",
        "iionly/tidypics": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    }
}
